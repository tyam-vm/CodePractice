// Problem34
//34A - Reconnaissance 2
#include <iostream>
using namespace std;
void solve();

void solve()
{int n;
cin >>n;
for( int i(0), idxMin(1), idxMin2(1) , temp , min(1000); i<n; i++)
{cin >> temp;
if(min >= temp){ min = temp; idxMin = idxMin2; idxMin2 =i+1;}
}
cout<< idxMin<< " "<<idxMin2;
}

int main()
{
int n(1);
// cin >> n;
while(n--)
{
 solve();
}
}
}
